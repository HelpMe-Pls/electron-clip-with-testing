{
  "name": "electron-clip",
  "productName": "Electron Clip",
  "version": "1.0.0",
  "description": "A menubar application with a rich UI.",
  "main": "app/main.js",
  "scripts": {
    "start": "electron .",
    "build": "npm run build-win",
    "build-mac": "electron-packager . --out=\"build\" --overwrite --asar",
    "build-win": "electron-packager . --out=\"build\" --overwrite --asar",
    "build-linux": "electron-packager . --out=\"build\" --overwrite --asar",
    "report": "node server.js",
    "test": "mocha"
  },
  "repository": "https://github.com/HelpMe-Pls/electron-clip-with-testing",
  "author": "Khoi Le <khoile5399@gmail.com>",
  "license": "MIT",
  "keywords": [
    "electron",
    "clipboard"
  ],
  "dependencies": {
    "electron": "3.0.10",
    "menubar": "5.2.3",
    "request": "2.88.0",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "multer": "^1.3.0",
    "uuid": "^3.1.0",
    "write-file": "^1.0.0"
  },
  "devDependencies": {
    "eslint": "5.10.0",
    "eslint-plugin-import": "2.14.0",
    "mocha": "5.2.0",
    "electron-packager": "^13.0.0",
    "spectron": "5.0.0"
  }
}